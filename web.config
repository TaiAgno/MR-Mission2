<configuration>
  <system.webServer>
    <handlers>
      <add name="iisnode" path="upload.js" verb="*" modules="iisnode"/>
    </handlers>
    <rewrite>
      <rules>
        <rule name="DynamicContent">
          <match url="/*" />
          <action type="Rewrite" url="upload.js"/>
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>